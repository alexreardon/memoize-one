{
  "presets": [
    "@babel/preset-flow",
    ["@babel/preset-env", {
      "loose": true
    }]
  ],
  "comments": false,
  "env": {
    "cjs": {
      "plugins": [
        "@babel/plugin-transform-es2015-modules-commonjs"
      ]
    },
    "test": {
      "plugins": [
        "@babel/plugin-transform-es2015-modules-commonjs"
      ]
    }
  }
}